    "version": "0.2.0",
    "configurations": [
      {
        "name": "Python: Current File",
        "type": "python",
        "request": "launch",
        "program": "${file}",
        "console": "integratedTerminal",
        "justMyCode": false,
        "env": {
          "PYTHONPATH": "${workspaceFolder}"
        }
      },
      {
        "name": "test",
        "type": "python",
        "request": "launch",
        "module": "unittest",
        "args": ["tests.app.internal.providers.ncc.test_provider"],
        "jinja": true,
        "justMyCode": false,
        "cwd": "${workspaceFolder}",
        "env": {
          // "LOG_FILE_PATH": ".logs",
          "PYTHONPATH": "${workspaceFolder}"
        },
        "envFile": "${workspaceFolder}/.env"
      }
    ]
  }